<!doctype html>
<html lang="es">
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<meta name="description" content="">
		<meta name="author" content="Mark Otto, Jacob Thornton, and Bootstrap contributors">
		<title>{{ page.title }}</title>

		<base
		href="http://localhost/registro-notas/" target="_self">

		<!-- Bootstrap core CSS -->
		<link href="./node_modules/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
		<script defer src="./node_modules/@fortawesome/fontawesome-free/js/all.min.js"></script>
		<meta
		name="theme-color" content="#563d7c">

		<!-- Custom styles for this template -->
		<link href="./src/css/style.css" rel="stylesheet">
		<link href="//cdn.datatables.net/1.10.24/css/jquery.dataTables.min.css" rel="stylesheet">
		<link href="https://cdn.datatables.net/buttons/1.7.0/css/buttons.dataTables.min.css" rel="stylesheet">
	</head>
	<body>

		{% block navbar %}
			{% include 'bloques/navbar.twig' %}
		{% endblock %}


		<main role="main">
			{% block contenido %}
				{% include 'bloques/contenido.twig' %}
			{% endblock %}
		</main>
		{% block footer %}
			<footer class="container">

			<br>
			
				<p>&copy; Jorge Gaitan-2021</p>
			</footer>
		{% endblock %}
		<script src="./node_modules/jquery/dist/jquery.min.js"></script>
		<script src="./node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
		{% block scripts %}
			<script src="./src/js/script.js"></script>
			<script src="//cdn.datatables.net/1.10.24/js/jquery.dataTables.min.js"></script>
			<script src="https://cdn.datatables.net/buttons/1.7.0/js/dataTables.buttons.min.js"></script>
			<script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
			<script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
			<script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
			<script src="https://cdn.datatables.net/buttons/1.7.0/js/buttons.html5.min.js"></script>
			<script src="https://cdn.datatables.net/buttons/1.7.0/js/buttons.print.min.js"></script>


		{% endblock %}
	</body>
</html>
