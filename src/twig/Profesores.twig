{% extends "base.twig" %}

{% block contenido %}
	<div class="container">
		<h1>Lista de profesores</h1>
        <table class="table">
			<thead>
				<tr>
					<th scope="col">ID</th>
					<th scope="col">Nombre</th>
				</tr>
			</thead>
			<tbody>
				{% for profesores in listaProfesores %}
					<tr>
						<th scope="row">{{ profesores.id }}</th>
						<td>{{ profesores.nombre }}</td>
					</tr>
				{% endfor %}
			</tbody>
		</table>

	</div>
{% endblock %}